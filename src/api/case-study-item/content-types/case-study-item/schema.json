{
  "kind": "collectionType",
  "collectionName": "case_study_items",
  "info": {
    "singularName": "case-study-item",
    "pluralName": "case-study-items",
    "displayName": "CaseStudyItem"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "text"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "summary": {
      "type": "text"
    },
    "client": {
      "type": "string"
    },
    "category": {
      "type": "string"
    },
    "case_study_services": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::case-study-service.case-study-service",
      "mappedBy": "case_study_item"
    },
    "content": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "section.case-study-result",
        "shared.card"
      ]
    }
  }
}
