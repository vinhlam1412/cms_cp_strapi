{
  "kind": "collectionType",
  "collectionName": "news_items",
  "info": {
    "singularName": "news-item",
    "pluralName": "news-items",
    "displayName": "NewsItem"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "text"
    },
    "summary": {
      "type": "text"
    },
    "category": {
      "type": "string"
    },
    "shareButton": {
      "type": "component",
      "component": "shared.logo-link",
      "repeatable": true
    },
    "content": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    }
  }
}
