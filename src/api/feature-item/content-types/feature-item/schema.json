{
  "kind": "collectionType",
  "collectionName": "feature_items",
  "info": {
    "singularName": "feature-item",
    "pluralName": "feature-items",
    "displayName": "FeatureItem"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "text"
    },
    "summary": {
      "type": "text"
    },
    "category": {
      "type": "string"
    },
    "shareButton": {
      "type": "component",
      "component": "shared.logo-link",
      "repeatable": true
    },
    "content": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "related_feature_item": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::feature-item.feature-item"
    }
  }
}
